{
    "global": {
        "uploadPath": "/Users/pantao/Desktop/upload",
        "allowRegister": true,
        "allowLogin": true,
        "loadParticle": false,
        "anonymousUser": {
            "visible": true,
            "downloadable": false
        }
    },
    "user": {
        "pageSize": 30,
        "password": {
            "minLength": 6,
            "maxLength": 20
        },
        "email": {
            "verify": false,
            "host": "mx1.hostinger.com.hk",
            "port": 587,
            "name": "efo",
            "from": "efo-admin@fakedog.xyz",
            "key": "ZehVZQMvhsXT72jf"
        },
        "usernameMatch": {
            "pattern": "[a-zA-Z][a-zA-Z0-9]{3,15}",
            "description": "用户名只能使用英文字母和数字，且必须以字母开始，长度为4到16的字符串"
        },
        "defaultAuth": {
            "isDownloadable": true,
            "isUploadable": true,
            "isDeletable": false,
            "isUpdatable": false,
            "isVisible": true
        },
        "orderBy": "id desc"
    },
    "download": {
        "pageSize": 30,
        "orderBy": "id desc"
    },
    "auth": {
        "pageSize": 30,
        "orderBy": "id desc",
        "default": {
            "isDownloadable": true,
            "isUploadable": true,
            "isDeletable": true,
            "isUpdatable": true,
            "isVisible": true
        }
    },
    "file": {
        "pageSize": 30,
        "maxSize": "1024 MB",
        "tag": {
            "require": false,
            "maxLength": 5,
            "maxSize": 5
        },
        "orderBy": "id desc",
        "coverIfExists": false,
        "linkRule": {
            "custom": "/{year}/{month}/{day}/{author}/{fileName}",
            "support": [
                "{year}",
                "{month}",
                "{day}",
                "{author}",
                "{fileName}",
                "{date}",
                "{categoryName}",
                "{fileId}"
            ]
        },
        "suffixMatch": {
            "pattern": "[a-zA-Z0-9]{0,10}",
            "description": "文件后缀只能包括数字和字母且长度不超过10"
        },
        "defaultAuth": {
            "isDownloadable": true,
            "isUploadable": true,
            "isDeletable": true,
            "isUpdatable": true,
            "isVisible": true
        }
    }
}